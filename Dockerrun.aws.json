{
	"AWSEBDockerrunVersion":2,
	"containerDefinitions":[
		{
			"name": "client",
			"image":"szt0172688/multi-client",
			"hostname":"client",
			"essential":false,
			"memory":128
		},
		{
			"name":"server",
			"image":"szt0172688/multi-server",
			"hostname":"api",
			"essential":false,
			"memory":128
		},
		{
			"name":"worker",
			"image":"szt0172688/multi-worker",
			"hostname":"worker",
			"essential":false,
			"memory":128
		},
		{
			"name":"nginx",
			"image":"szt0172688/multi-nginx",
			"portMappings":[
				{"hostPort":80,
				"containerPort":80}
			],
			"link":["client","server"],
			"hostname":"nginx",
			"essential":true,
			"memory":128
		}

	]
}